  "compilerOptions": {
    "noImplicitAny": true,
    "target": "es5",
    "lib": ["es5", "es6", "esnext", "dom"],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "esModuleInterop": true
  }
}
